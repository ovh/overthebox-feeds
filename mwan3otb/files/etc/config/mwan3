
config rule 'voip'
	option dest_ip '91.121.128.0/23'
	option proto 'udp'
	option sticky '0'
	option use_policy 'failover'

config rule 'icmp'
	option proto 'icmp'
	option sticky '0'
	option use_policy 'failover'

config rule 'openvpn'
	option dest_port '1194:1196'
	option sticky '0'
	option use_policy 'balanced'
	option proto 'tcp'

config rule 'socks'
	option dest_port '1090'
	option proto 'tcp'
	option sticky '0'
	option use_policy 'balanced'

config rule 'all'
	option proto 'all'
	option sticky '0'

config interface 'tun0'
	option enabled '1'
	list track_ip '10.166.177.1'
	option reliability '1'
	option count '1'
	option timeout '2'
	option interval '5'
	option down '3'
	option up '3'
